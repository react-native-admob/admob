{
  "name": "@react-native-admob/admob",
  "version": "1.2.0",
  "description": "Admob for React Native with powerful hooks and components",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "RNAdMob.podspec",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "lint:js": "eslint --ext '.js,.ts,.tsx' src/ && yarn prettier --check src/",
    "release": "release-it",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "admob"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-native-admob/admob.git"
  },
  "author": "Jay Kim <me@wjay.kim> (https://github.com/wjaykim)",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/react-native-admob/admob/issues"
  },
  "homepage": "https://github.com/react-native-admob/admob#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/react-native": "^0.66.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-native": "0.66.1",
    "react-native-builder-bob": "^0.18.2",
    "release-it": "^14.11.6",
    "typescript": "^4.4.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
